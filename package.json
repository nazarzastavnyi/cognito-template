{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seeder:run": "node dist/seeder",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "secrets": "ts-node ./scripts/secrets.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.272.0",
    "@aws-sdk/client-lambda": "^3.338.0",
    "@aws-sdk/client-secrets-manager": "^3.272.0",
    "@aws-sdk/client-sqs": "^3.370.0",
    "@aws-sdk/smithy-client": "^3.272.0",
    "@fast-csv/parse": "^4.3.6",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/body-parser": "^1.19.2",
    "aws-jwt-verify": "^4.0.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parser": "^3.0.0",
    "envfile": "^6.18.0",
    "husky": "^8.0.3",
    "jwks-rsa": "^3.0.0",
    "nest-boilerplate": "link:",
    "nest-csv-parser": "^2.0.4",
    "openpgp": "^5.11.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pino-http": "^8.3.0",
    "pnpm": "^7.18.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0",
    "streamifier": "^0.1.1",
    "underscore": "^1.13.6",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.14",
    "@types/jest": "29.2.4",
    "@types/node": "^18.11.11",
    "@types/streamifier": "^0.1.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nestjs-seeder": "^0.3.0",
    "prettier": "^2.8.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "rootDir": "src",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/$1",
      "^@api/(.*)$": "<rootDir>/api/$1",
      "^@common/(.*)$": "<rootDir>/common/$1"
    }
  }
}
